{
    "extends": [
      "airbnb",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "babelOptions": {
        "presets": ["@babel/preset-react"]
      }
    },
  
    "plugins": ["prettier"],
    "rules": {
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/label-has-associated-control": "off",
      "react/prop-types": 0,
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
}